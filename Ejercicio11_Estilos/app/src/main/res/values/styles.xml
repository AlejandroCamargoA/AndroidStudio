<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="GreenText" parent="TextAppearance.AppCompat">
        <item name="android:textColor">#00FF00</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textSize">32sp</item>
    </style>
    <style name="textoAlerta" parent="TextAppearance.AppCompat">
        <item name="android:textColor">#FF0000</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">15sp</item>
    </style>
    <style name="botonNormal" parent="TextAppearance.AppCompat">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textSize">15sp</item>
        <item name="android:background">#FF9800</item>
        <item name="android:textAllCaps">true</item>
    </style>
    <style name="textoNormal" parent="TextAppearance.AppCompat">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">12sp</item>
    </style>
</resources>